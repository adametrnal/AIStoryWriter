    "@supabase/supabase-js": "^2.47.1",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "expo": "^52.0.11",
    "expo-auth-session": "~6.0.0",
    "expo-av": "~15.0.1",
    "expo-constants": "~17.0.3",
